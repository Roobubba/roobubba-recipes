<%= render 'shared/page_title', title: "All Chefs" %>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-8 col-md-offset-2 center">
      <%= will_paginate %>
      <br>
  
  
      <% @chefs.each do |chef| %>
      
            <%= link_to gravatar_for(chef, size: 200), chef_path(chef) %>
            <p>
              <h5>Chef name: <%= chef.chefname %></h5>
            </p>
      
      
      <% end %>
      
      <%= will_paginate %>
    </div>
  </div>
</div>