<div class="container-fluid">	
  <nav class="navbar navbar-inverse">
    <div class="navbar-header">
  		<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse-1" aria-expanded="false">
  			<span class="sr-only">Toggle navigation</span>
  			<span class="icon-bar"></span>
  			<span class="icon-bar"></span>
  			<span class="icon-bar"></span>
  		</button>
  		<%= link_to "RooBubba Recipes", root_path, class: "navbar-brand logo" %>
  	</div>
  	<!-- Collect the nav links, forms, and other content for toggling -->
  	<div class="collapse navbar-collapse" id="navbar-collapse-1">
			<ul class="nav navbar-nav">	
				<li><%= link_to "View Recipes", recipes_path %></li>
				<% if logged_in? %><li><%= link_to "New Recipe", new_recipe_path %></li><% end %>
				<li><%= link_to "About Us", "" %></li>
  	  </ul>
  	  <div class="pull-right">
  	  	<ul class="nav navbar-nav nav-item">
  	  		<% if !logged_in? %>
  	  			<li>
  	  				<%= link_to "Log In", login_path %>
  	  			</li>
  	  		<% else %>
  	  			<li class="dropdown">
  	  				<%= link_to '#', class: "dropdown-toggle", "data-toggle" => "dropdown" do %>
  	  					<%= current_user.chefname %><b class="caret"></b>
  	  				<% end %>
  	  					<ul class="dropdown-menu">
  	  						<li><%= link_to "View profile", chef_path(current_user) %></li>
  	  						<li><%= link_to "Edit profile", edit_chef_path(current_user) %></li>
  	  						<li class="divider"></li>
  	  						<li><%= link_to "Log Out", logout_path %></li>
  	  				</ul>
  	  			</li>
  	  		<% end %>
  			</ul>
 			</div>
  	</div><!-- /.navbar-collapse -->
	</nav>
